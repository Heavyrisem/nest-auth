{
    "$schema": "https://json.schemastore.org/swcrc",
    "exclude": [
        "node_modules",
        "jest.config.ts"
    ],
    "jsc": {
        "target": "es2020",
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": false
        },
        "externalHelpers": false
    },
    "module": {
        "type": "commonjs"
    }
}